{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "anhskohbo/no-captcha": "^3.5",
        "aws/aws-php-sns-message-validator": "^1.8",
        "aws/aws-sdk-php": "^3.263",
        "barryvdh/laravel-dompdf": "^2.0",
        "bensampo/laravel-enum": "^6.2",
        "cloudinary-labs/cloudinary-laravel": "^2.0",
        "fruitcake/laravel-cors": "^3.0",
        "galbar/jsonpath": "^2.1",
        "grosv/laravel-passwordless-login": "^1.8",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.4",
        "jd-dotlogics/laravel-grapesjs": "^3.4",
        "jorenvanhocht/laravel-share": "^4.2",
        "kub-at/php-simple-html-dom-parser": "^1.9",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "league/csv": "^9.8",
        "league/pipeline": "^1.0",
        "mailgun/mailgun-php": "^3.5",
        "mews/purifier": "^3.3",
        "omnipay/paypal": "^3.0",
        "omnipay/sagepay": "~3.0",
        "php-http/client-common": "^2.6",
        "php-http/curl-client": "^2.2",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/httplug": "^2.3",
        "php-http/message": "^1.13",
        "pusher/pusher-php-server": "^7.2",
        "realrashid/sweet-alert": "^5.1",
        "sendgrid/sendgrid": "^8.0",
        "sendgrid/smtpapi": "^0.7.0",
        "sparkpost/sparkpost": "^2.3",
        "spatie/laravel-permission": "^5.5",
        "srmklive/paypal": "^3.0",
        "stevebauman/location": "^7.0",
        "stripe/stripe-php": "^13.5",
        "swiftmailer/swiftmailer": "^6.0",
        "twig/twig": "^3.5",
        "twilio/sdk": "^6.43",
        "unicodeveloper/laravel-paystack": "1.0.*",
        "zeevx/lara-termii": "dev-master"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^6.7",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/namespaced_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
